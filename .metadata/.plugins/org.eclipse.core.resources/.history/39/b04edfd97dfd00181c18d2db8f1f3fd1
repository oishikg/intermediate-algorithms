package sorting;


public class PriorityQueue 
{
	/* instance variables: note that we only implement a pq for integers, 
	 * and a variable N to keep track of the size of the pq */
	int[] pq; 
	int N;
	
	/* constructor to instantiate the priority queue with a client provided 
	 * initial size*/
	public PriorityQueue (int inSz)
	{
		
	}
	private void resize(int max)
	{
		int [] temp = new int[max];
		for (int i=0; i<N ; i++)
			temp[i] = pq [i];
		a = temp;
	}
}
